              RowBox[{"v", "==", "0."}]}], ",", 
             RowBox[{"EpsSpecies", "=", 
              RowBox[{"SigmaCold", "[", 
               RowBox[{"fp", ",", "fc", ",", "freq"}], "]"}]}]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{
               "model", "\[LeftDoubleBracket]", "i", 
                "\[RightDoubleBracket]"}], "==", "2"}], "&&", 
              RowBox[{"v", ">", "0"}]}], ",", 
             RowBox[{
              RowBox[{"Do", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"SigmaSpecies", "=", 
                  RowBox[{"SigmaMaxn", "[", 
                   RowBox[{
                   "fp", ",", "fc", ",", "freq", ",", "v", ",", "nz", ",", 
