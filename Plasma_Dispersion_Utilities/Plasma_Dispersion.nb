          RowBox[{
           RowBox[{"eta", "[", "i", "]"}], ">", "0."}], ",", 
          RowBox[{
           RowBox[{"fp", "=", 
            RowBox[{"N", "[", 
             FractionBox[
              RowBox[{"8.98", " ", 
               SqrtBox[
                FractionBox[
                 RowBox[{"n0", " ", 
                  RowBox[{"eta", "[", "i", "]"}], " ", 
