    RowBox[{
     RowBox[{"b", "=", 
      FractionBox[
       RowBox[{"nx", " ", "v", " ", "f"}], 
       RowBox[{"fc", " ", 
        SqrtBox["2."]}]]}], ";", 
     RowBox[{"gamma", "=", 
      SuperscriptBox["b", "2"]}], ";", 
     RowBox[{"z", "=", 
      FractionBox[
       RowBox[{"1", "-", 
        FractionBox[
         RowBox[{"n", " ", "fc"}], "f"]}], 
       RowBox[{"nz", " ", "v"}]]}], ";", 
     RowBox[{"z0", "=", 
      FractionBox["1", 
       RowBox[{"nz", " ", "v"}]]}], ";", 
     RowBox[{"Zf", "=", 
