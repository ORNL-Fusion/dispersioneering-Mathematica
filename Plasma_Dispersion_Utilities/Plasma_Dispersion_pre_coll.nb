                 RowBox[{"EpsSpecies", "=", 
                  RowBox[{"EpsSpecies", "+", "SigmaSpecies"}]}], ";"}], ",", 
                RowBox[{"{", 
                 RowBox[{"nHarmonic", ",", 
                  RowBox[{
                  "nmin", "\[LeftDoubleBracket]", "i", 
                   "\[RightDoubleBracket]"}], ",", 
                  RowBox[{
                  "nmax", "\[LeftDoubleBracket]", "i", 
                   "\[RightDoubleBracket]"}]}], "}"}]}], "]"}], ";"}]}], 
            "]"}], ";", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{
               "model", "\[LeftDoubleBracket]", "i", 
                "\[RightDoubleBracket]"}], "\[Equal]", "2"}], "&&", 
              RowBox[{"v", "==", "0."}]}], ",", 
