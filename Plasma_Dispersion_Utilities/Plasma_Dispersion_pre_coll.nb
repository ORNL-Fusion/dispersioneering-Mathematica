  RowBox[{"SigmaMaxn", "[", 
   RowBox[{
   "fp_", ",", "fc_", ",", "f_", ",", "v_", ",", "nz_", ",", "nx_", ",", 
    "n_Integer"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "z", ",", "z0", ",", "Z", ",", "b", ",", "gamma", ",", "eIbess", ",", 
