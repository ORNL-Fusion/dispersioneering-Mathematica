               RowBox[{"fp", ",", "fc", ",", "freq"}], "]"}]}]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"print", "\[Equal]", "True"}], ",", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"Print", "[", 
                RowBox[{
                "\"\<\\n\\nEpsGeneral    species=\>\"", ",", "i", ",", 
                 "\"\<    model for this species =\>\"", ",", 
                 RowBox[{
                 "model", "\[LeftDoubleBracket]", "i", 
                  "\[RightDoubleBracket]"}]}], "]"}], ";", 
               RowBox[{"Print", "[", 
                RowBox[{"\"\<Eps[i]=\>\"", ",", 
                 RowBox[{"Chop", "[", "EpsSpecies", "]"}]}], "]"}]}], ")"}]}],
             "]"}], ";"}]}], "]"}], ";", 
        RowBox[{"Eps", "=", 
         RowBox[{"Eps", "+", "EpsSpecies"}]}], ";"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", "6"}], "}"}]}], "]"}], ";", "Eps"}]}], 
